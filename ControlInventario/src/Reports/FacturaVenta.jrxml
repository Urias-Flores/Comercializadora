	<parameter name="VentaID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
v.VentaID as NoFactura,
u.Nombre as Usuario,
c.Nombre as Cliente,
v.Fecha,
v.Hora,
p.ProductoID as Codigo,
p.Descripcion as Producto,
vd.Cantidad,
vd.Precio,
vd.ISV,
vd.Descuento
FROM Venta AS v
INNER JOIN VentaDetalle AS vd ON vd.CompraID = v.VentaID
INNER JOIN Producto as p ON p.ProductoID = vd.ProductoID
INNER JOIN Cliente as c ON c.ClienteID = v.ClienteID
INNER JOIN Usuario as u ON u.UsuarioID = v.UsuarioID
WHERE v.VentaID = 0]]>
	</queryString>
